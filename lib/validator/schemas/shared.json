{
    "type": "object",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "properties": {
        "cda_address": {
            "type": "object",
            "properties": {
                "city": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "streetLines": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1
                },
                "use": {
                    "type": "string"
                },
                "zip": {
                    "type": "string"
                }
            },
            "required": [
                "streetLines",
                "city"
            ]
        },
        "additionalProperties": false,
        "cda_date": {
            "type": "object",
            "properties": {
                "low": {
                    "$ref": "http://local.com/common_models#/properties/cda_date_element"
                },
                "point": {
                    "$ref": "http://local.com/common_models#/properties/cda_date_element"
                },
                "high": {
                    "$ref": "http://local.com/common_models#/properties/cda_date_element"
                }
            },
            "additionalProperties": false
        },
        "cda_date_element": {
            "type": "object",
            "properties": {
                "date": {
                    "type": "string",
                    "format": "date-time"
                },
                "precision": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "date"
            ]
        },
        "cda_phone": {
            "type": "object",
            "properties": {
                "number": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "number"
            ]
        },
        "cda_email": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "address"
            ]
        },
        "cda_id": {
            "type": "object",
            "properties": {
                "identifier_type": {
                    "type": "string"
                },
                "identifier": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "identifier"
            ]
        },
        "cda_name": {
            "type": "object",
            "properties": {
                "prefix": {
                    "type": "string"
                },
                "first": {
                    "type": "string"
                },
                "last": {
                    "type": "string"
                },
                "middle": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "suffix": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "first",
                "last"
            ]
        },
        "cda_coded_entry": {
            "type": "object",
            "properties": {
                "code_system_name": {
                    "type": "string"
                },
                "code": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "translations": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "code_system_name": {
                                "type": "string"
                            },
                            "code": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "additionalProperties": false,
                        "minProperties": 1
                    },
                    "minItems": 1
                }
            },
            "minProperties": 1,
            "additionalProperties": false
        },
        "cda_physical_quantity": {
            "type": "object",
            "properties": {
                "unit": {
                    "type": "string"
                },
                "value": {
                    "type": "number"
                }
            },
            "additionalProperties": false,
            "required": [
                "unit",
                "value"
            ]
        },
        "cda_location": {
            "type": "object",
            "properties": {
                "addresses": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_address"
                    },
                    "minItems": 1
                },
                "loc_type": {
                    "$ref": "#/properties/cda_coded_entry"
                },
                "name": {
                    "type": "string"
                },
                "phones": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_phone"
                    }
                }
            },
            "required": [
                "name"
            ],
            "additionalProperties": false
        },
        "cda_organization": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "array",
                    "items": {
                        "$ref": "http://local.com/common_models#/properties/cda_address"
                    }
                },
                "identifiers": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_id"
                    }
                },
                "name": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "phone": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_phone"
                    }
                },
                "email": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_email"
                    }
                },
                "phone": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_phone"
                    }
                }
            }
        },
        "cda_performer": {
            "type": "object",
            "properties": {
                "type": "string",
                "performer": {
                    "$ref": "#/properties/cda_coded_entry"
                },
                "address": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_address"
                    }
                },
                "identifiers": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_id"
                    }
                },
                "name": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_name"
                    }
                },
                "organization": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_organization"
                    }
                },
                "phone": {
                    "type": "array",
                    "items": {
                        "$ref": "#/properties/cda_phone"
                    }
                }
            },
            "required": [
                "address"
            ]
        }
    }
}
